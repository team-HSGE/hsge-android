<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.starters.hsge.presentation.main.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/findFragment"
        android:name="com.starters.hsge.presentation.main.find.FindFragment"
        android:label="fragment_find"
        tools:layout="@layout/fragment_find" />
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.starters.hsge.presentation.main.chat.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" />
    <fragment
        android:id="@+id/myPageFragment"
        android:name="com.starters.hsge.presentation.main.mypage.MyPageFragment"
        android:label="fragment_my_page"
        tools:layout="@layout/fragment_my_page">
        <action
            android:id="@+id/action_myPageFragment_to_userProfileEditFragment"
            app:destination="@id/userProfileEditFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_userDistanceFragment"
            app:destination="@id/userDistanceFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_userLocationFragment2"
            app:destination="@id/userLocationFragment2" />
        <action
            android:id="@+id/action_myPageFragment_to_managementFragment"
            app:destination="@id/managementFragment" />
    </fragment>
    <fragment
        android:id="@+id/userProfileEditFragment"
        android:name="com.starters.hsge.presentation.main.mypage.UserProfileEditFragment"
        android:label="fragment_user_profile_edit"
        tools:layout="@layout/fragment_user_profile_edit" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.starters.hsge.presentation.main.mypage.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settingsFragment_to_withdrawalFragment"
            app:destination="@id/withdrawalFragment" />
    </fragment>
    <fragment
        android:id="@+id/withdrawalFragment"
        android:name="com.starters.hsge.presentation.main.mypage.WithdrawalFragment"
        android:label="fragment_withdrawal"
        tools:layout="@layout/fragment_withdrawal" />
    <fragment
        android:id="@+id/managementFragment"
        android:name="com.starters.hsge.presentation.main.management.ManagementFragment"
        android:label="ManagementFragment" >
        <action
            android:id="@+id/action_managementFragment_to_dogProfileEditFragment"
            app:destination="@id/dogProfileEditFragment" />
    </fragment>
    <fragment
        android:id="@+id/dogProfileEditFragment"
        android:name="com.starters.hsge.presentation.main.edit.DogProfileEditFragment"
        android:label="DogProfileEditFragment" />

    <fragment
        android:id="@+id/userDistanceFragment"
        android:name="com.starters.hsge.presentation.main.mypage.userDistance.UserDistanceFragment"
        android:label="UserDistanceFragment" >
        <action
            android:id="@+id/action_userDistanceFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/userLocationFragment2"
        android:name="com.starters.hsge.presentation.register.fragment.userLocation.UserLocationFragment"
        android:label="UserLocationFragment"
        tools:layout="@layout/fragment_user_location">
    </fragment>
</navigation>